<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">

    <% if (theme.favicon_url) { %>
      <link rel="icon" href="<%- url_for(theme.favicon_url) %>" />
    <% } %>

    <title>
        <% if (page.title) { %>
          <%= page.title + ' - ' + config.title %>
        <% } else{%>
          <%= config.title %>
        <% } %>
    </title>

    <!-- Spectre.css framework -->
    <link rel="stylesheet" href="spectre-0.5.9/spectre.min.css">
    <link rel="stylesheet" href="spectre-0.5.9/spectre-exp.min.css">
    <link rel="stylesheet" href="spectre-0.5.9/spectre-icons.min.css">

    <!-- theme css & js -->
    <%- css('css/book') %>
    <%- js('js/book') %>

    <!-- tocbot -->
    <script src="tocbot-4.12.0/tocbot.min.js"></script>
    <link rel="stylesheet" href="tocbot-4.12.0/tocbot.css">
    
    <!-- katex -->
    <link rel="stylesheet" href="katex/katex.min.css">

    <%- partial('_lib/google-analytics') %>
    <%- partial('_lib/zooming-image') %>
</head>